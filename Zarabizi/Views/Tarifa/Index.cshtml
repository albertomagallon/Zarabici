@model IEnumerable<Zarabizi.Models.Tarifa>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            diasTarifa
        </th>
        <th>
            nombreTarifa
        </th>
        <th>
            fechaInicioTarifa
        </th>
        <th>
            fechaFinalTarifa
        </th>
        <th>
            precioTarifa
        </th>
        <th>
            Oficina
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.diasTarifa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombreTarifa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaInicioTarifa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaFinalTarifa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.precioTarifa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Oficina.nombreOficina)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idTarifa }) |
            @Html.ActionLink("Details", "Details", new { id=item.idTarifa }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idTarifa })
        </td>
    </tr>
}

</table>
