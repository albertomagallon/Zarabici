    <div class="display-field">
        @Html.DisplayFor(model => model.fechaNacimientoSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_fechaAltaSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.fechaAltaSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_CuentaBancariaSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CuentaBancariaSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_idOficina</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Oficina.nombreOficina)
    </div>

    <div class="display-label">@Messages.form_Socio_idUsuario</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.aspnet_Users.UserName)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Eliminar" /> |
        @Html.ActionLink(@Messages.lbl_VolverLista, "Index")
    </p>
}
