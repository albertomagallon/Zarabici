@model Zarabizi.Models.Socio
@using Zarabizi.Resources          

@{
    ViewBag.Title = @Messages.lbl_Eliminar;
}

<h2>@Messages.lbl_Eliminar</h2>

<h3>@Messages.lbl_SeguroEliminarSocio</h3>
<fieldset>
    <legend>@Messages.lbl_Socio</legend>

    <div class="display-label">@Messages.form_Socio_nombreSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.nombreSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_apellidosSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.apellidosSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_direccionSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.direccionSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_poblacionSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.poblacionSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_codigopostalSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.codigopostalSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_tlfFijoSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.tlfFijoSocio)
    </div>

    <div class="display-label">@Messages.form_Socio_tlfMovilSocio</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.tlfMovilSocio)
