@model Zarabizi.Models.Oficina

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Oficina</legend>

        @Html.HiddenFor(model => model.idOficina)

        <div class="editor-label">
            @Html.LabelFor(model => model.nombreOficina)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.nombreOficina)
            @Html.ValidationMessageFor(model => model.nombreOficina)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ubicacionOficina)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ubicacionOficina)
            @Html.ValidationMessageFor(model => model.ubicacionOficina)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ciudadOficina)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ciudadOficina)
            @Html.ValidationMessageFor(model => model.ciudadOficina)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.provinciaOficina)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.provinciaOficina)
            @Html.ValidationMessageFor(model => model.provinciaOficina)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
